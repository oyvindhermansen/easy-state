{
  "name": "setState",
  "version": "0.1.0",
  "description": "Simpler state manipulation without any frameworks",
  "main": "./lib/index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublish": "npm run build",
    "dev": "webpack-dev-server --hot --inline --progress --colors --open",
    "build": "NODE_ENV=production cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/index.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oyvindhermansen/setState.git"
  },
  "keywords": [
    "state",
    "setState",
    "state manipulation"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^4.0.0",
    "husky": "^0.13.4",
    "jest": "^19.0.2",
    "jquery": "^3.2.1",
    "lint-staged": "^3.6.1",
    "prettier": "^1.4.4",
    "webpack": "latest",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "redux": "^3.7.2"
  }
}
